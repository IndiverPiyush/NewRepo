@model IList<MedicineSearchWebApp.Models.MedecineADO>
<div id="MEDS">

    <table class="table">
        <thead>
            <tr>
                <th style="background-color : black">
                    Medicine Name
                </th>
                <th style="background-color : black">
                    @Html.ActionLink("CATEGORY", "Index", new { sort = ViewBag.CatSortParam })
                </th>
                <th style="background-color : black">
                    Dosage
                </th>
                <th style="background-color : black">
                    @Html.ActionLink("MANUFACTURING DATE", "Index", new { sort = ViewBag.MDATESortParam })
                </th>
                <th style="background-color : black">
                    @Html.ActionLink("EXPIRY DATE", "Index", new { sort = ViewBag.EDATESortParam })
                </th>
                <th style="background-color : black">
                    @Html.ActionLink("STOCK", "Index", new { sort = ViewBag.StockSortParam })
                </th>
                <th style="background-color : black">
                    @Html.ActionLink("PRICE", "Index", new { sort = ViewBag.PriceSortParam })
                </th>
                <th style="background-color : black">
                    Supplier
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.MedicineName)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.MedicineCategory)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.MedicineDosage)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.MedicineMdate)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.MedicineEdate)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.MedicineStock)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.MedicinePrice)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.ProviderId)
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>