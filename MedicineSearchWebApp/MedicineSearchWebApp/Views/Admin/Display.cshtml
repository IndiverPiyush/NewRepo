@model IEnumerable<MedicineSearchWebApp.Models.Vendor>

@{
    ViewData["Title"] = "Display";
}

<h1>Display</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.VendorOrgName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.VendorArea)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.VendorCity)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.VendorMobile)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.VendorWallet)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.VendorPassword)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.VendorSpeciality)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.VendorOrgName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.VendorArea)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.VendorCity)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.VendorMobile)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.VendorWallet)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.VendorPassword)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.VendorSpeciality)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.VendorId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.VendorId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.VendorId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
